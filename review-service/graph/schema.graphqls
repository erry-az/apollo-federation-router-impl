# GraphQL schema example
#
# https://gqlgen.com/getting-started/

extend type User @key(fields: "id") {
  id: ID! @external
  reviews: [Review]
  totalReview: Int
}

type Review @key(fields: "id") {
  id: ID!
  title: String
  description: String
  reviewer: User
}

type ReviewListResponse {
  reviews: [Review]
}

type Query {
  ReviewList: [Review]
}

